<?xml version="1.0" encoding="utf-8"?>
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="34" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:label="{{ .AppName }}" android:icon="@drawable/icon" android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity" android:exported="true" android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>